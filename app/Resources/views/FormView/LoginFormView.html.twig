{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">
        {% if error %}
            <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
        {% endif %}

        {{ form_start(form) }}
        {{ form_widget(form) }}
        <input type="submit" class="btn btn-primary" formnovalidate="formnovalidate" value="Submit" />
        {{ form_end(form) }}
    </div>
{% endblock %}