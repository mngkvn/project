{% extends 'base.html.twig' %}

{% block body %}
    <table>
        {% for data in photoRequest %}
            <tr>
                <td><a href="{{ path("photoRequestView", {id: data.id}) }}">{{ data.id }}</a></td>
                <td>{{ data.name }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}